{
  "name": "placement-services-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=development nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --verbose --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "node dist/database/seeder",
    "seed:refresh": "node dist/database/seeder --refresh",
    "clear_jest": "jest --clearCache"
  },
  "dependencies": {
    "@casl/ability": "^6.4.0",
    "@golevelup/nestjs-stripe": "^0.6.5",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^1.4.2",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^10.0.1",
    "@recursyve/nestjs-quickbooks": "^8.0.0-beta.21",
    "@sendgrid/mail": "^8.1.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "btoa": "^1.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "^5.0.0",
    "client-oauth2": "^4.3.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "csrf": "^3.1.0",
    "express-ctx": "^0.1.1",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.1.4",
    "intuit-oauth": "^4.1.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.1",
    "node-fetch": "^3.3.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.8.0",
    "password-hash": "^1.2.2",
    "quickbooks": "^1.0.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.2.0",
    "stripe": "^11.18.0",
    "typeorm": "^0.3.19",
    "webpack": "^5.78.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/multer": "^1.4.7",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "rootDir": "src",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageReporters": [
      "json"
    ],
    "coverageDirectory": "../coverage"
  }
}
